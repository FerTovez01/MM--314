//2.26

import java.util.Scanner;

public class Multiplo
{
  public static void main (String[]args)
  {
    // Crear un objeto Scanner para la entrada del usuario
    Scanner entrada = new Scanner (System.in);

    // Pedir al usuario que ingrese dos enteros
      System.out.print ("Por favor, ingrese el primer nC:mero entero: ");
    int numero1 = entrada.nextInt ();

      System.out.print ("Por favor, ingrese el segundo nC:mero entero: ");
    int numero2 = entrada.nextInt ();

    // Verificar si el primer nC:mero es un mC:ltiplo del segundo
    if (numero1 % numero2 == 0)
      {
	System.out.println (numero1 + " es un mC:ltiplo de " + numero2);
      }
    else
      {
	System.out.println (numero1 + " no es un mC:ltiplo de " + numero2);
      }

    // Cerrar el objeto Scanner
    entrada.close ();
  }
}
